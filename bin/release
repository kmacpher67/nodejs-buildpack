#!/usr/bin/env bash
# bin/release <build-dir>

cat << EOF
addons: []
config_vars:
  PATH: "/usr/local/bin:/usr/bin:/bin:/app/vendor/xvfb/bin"
  LD_LIBRARY_PATH: "/usr/local/lib:/usr/lib:/lib:/app/vendor/xvfb/lib"
  TMPDIR: "/app/tmp"
default_process_types:
  web: npm start
  console: "vendor/xvfb/bin/xvfb"
EOF
